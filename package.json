{
  "name": "avaliacion-moderna",
  "version": "2.0.0",
  "description": "Sistema moderno de evaluación con React, TypeScript y TailwindCSS",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:full": "concurrently -k -n WEB,API -c blue,green \"npm:dev\" \"npm:server\"",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "node server/index.js",
    "start": "NODE_ENV=production node server/index.js",
    "db:seed:lomloe": "node server/database/seed_lomloe_from_csv.mjs server/database/seed_data/reglas-negocio.csv",
    "firestore:seed:demo": "node server/scripts/seed_firestore_demo.mjs",
    "firestore:reset:demo": "node server/scripts/reset_demo_workspace.mjs",
    "firestore:seed:demo:evidence": "node server/scripts/seed_demo_evidence.mjs",
    "firestore:seed:competencias": "node server/scripts/seed_competencias_workspace.mjs"
  },
  "keywords": [
    "educación",
    "evaluación",
    "react",
    "typescript",
    "tailwindcss"
  ],
  "author": "CEIP Galicia",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "cheerio": "^1.1.2",
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.19",
    "typescript": "^5.9.3",
    "vite": "^7.3.0"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "firebase": "^12.7.0",
    "firebase-admin": "^13.6.0",
    "lucide-react": "^0.561.0",
    "pg": "^8.16.3",
    "react": "^19.2.3",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0"
  }
}
